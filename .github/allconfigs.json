{
  "settings": {
    "lastCommit": ""
  },
  "outputs": {
    "auto": {
      "vars": {
        "branch": "auto-update",
        "branchPrefix": "chore/",
        "whitelist": [
          "dependabot[bot]",
          "dependabot-preview[bot]",
          "github-actions[bot]",
          "github-actions",
          "github-user",
          "actions-user",
          "web-flow",
          "VidendumStudios"
        ],
        "destination": "master",
        "body": ":crown: *An automated PR*",
        "labels": "skip-changelog,automerge",
        "commitOptions": "-S --sign-off",
        "pushOptions": ""
      }
    },
    "gpg": {
      "secret": false,
      "vars": {
        "signingKey": true,
        "commitSign": true,
        "tagSign": true,
        "pushSign": true
      }
    },
    "projects": {
      "secret": false,
      "vars": {
        "cardsConfig": "labeler-card.yml",
        "useOrg": "false ",
        "devOps": "1",
        "devOpsColumn": "Requested",
        "issues": "2",
        "issuesColumn": "New",
        "comOps": "3",
        "comOpsColumn": "Accepted"
      }
    },
    "cla": {
      "secret": false,
      "vars": {
        "path": ".github/cla.json",
        "docs": "docs/getting-started/contributing/cla.md",
        "label": true,
        "prtitle": "chore: CLA Management"
      }
    },
    "periodic": {
      "secret": false,
      "vars": {
        "path": ".github/labeler-path.yml"
      }
    },
    "stale": {
      "secret": false,
      "vars": {
        "stale-issue-message": "This issue has been automatically marked as stale because it has not had recent activity. It will be closed if no further activity occurs. Thank you for your contributions.",
        "stale-issue-label": "Status - Stale",
        "stale-pr-message": "Stale pull request message",
        "stale-pr-label": "Status - Abandoned",
        "exempt-issue-labels": "security vulnerability",
        "days-before-stale": "30",
        "days-before-close": "5"
      }
    },
    "profanity": {
      "secret": false
    },
    "title": {
      "secret": false,
      "vars": {
        "labelTitlePath": ".github/labeler-title.yml",
        "Labelversioned": "0"
      }
    },
    "sponsor": {
      "secret": false,
      "vars": {
        "Label": "Sponsor Request ❤️"
      }
    },
    "issueBranch": {
      "secret": false
    },
    "commands": {
      "secret": false,
      "vars": {
        "claim": true
      }
    },
    "wip": {
      "secret": false,
      "vars": {
        "label": true
      }
    },
    "semantic": {
      "secret": false
    },
    "size": {
      "secret": false,
      "vars": {
        "size_xs_label": "Size - XS",
        "size_s_label": "Size - S",
        "size_m_label": "Size - M",
        "size_l_label": "Size - L",
        "size_xl_label": "Size - XL",
        "size_xxl_label": "Size - XXL"
      }
    },
    "path": {
      "secret": false,
      "vars": {
        "config": ".github/labeler-path.yml"
      }
    },
    "branch": {
      "secret": false,
      "vars": {
        "config": ".github/labeler-branch.yml"
      }
    },
    "conflict": {
      "secret": false,
      "vars": {
        "master": true,
        "pull": true,
        "dirty": "Needs rebase"
      }
    },
    "approve": {
      "secret": false
    },
    "merge": {
      "secret": false,
      "vars": {
        "labels": "automerge",
        "deleteLabels": "automerge",
        "method": "squash",
        "forks": true,
        "retries": 6,
        "sleep": 5000,
        "delete": true
      }
    },
    "release": {
      "secret": false,
      "vars": {
        "major": "Versioning - Major",
        "minor": "Versioning - Minor",
        "patch": "Versioning - Patch"
      }
    },
    "changelog": {
      "secret": false,
      "vars": {
        "Config": "docs/information/changelog.md",
        "pullRequests": true,
        "prWoLabels": "skip-changelog",
        "breakingLabels": "Versioning - BREAKING",
        "enhancementLabels": "Type - Enhancement, Type - Feature",
        "bugLabels": "Type - Fix, Bug - Fixed",
        "deprecatedLabels": "Type - Decrecated",
        "removedLabels": "Type - Removal",
        "securityLabels": "security fix",
        "excludeLabels": "skip-changelog",
        "stripGeneratorNotice": "true"
      }
    },
    "copy": {
      "secret": false,
      "vars": {
        "targetBranch": "master"
      }
    },
    "wiki": {
      "secret": false,
      "vars": {
        "docsToWiki": true,
        "docsToWikiSource": "docs",
        "docsToWikiDestination": "wiki",
        "docsToRepo": true,
        "docsToRepoSource": "docs",
        "docsToRepoDestination": ".",
        "docsToRepoSuffix": "_wiki",
        "wikiToDocs": true,
        "wikiToDocsPRtitle": "docs: updating wikiToDocs",
        "wikiToRepo": true
      }
    },
    "labels": {
      "secret": false,
      "vars": {
        "config": ".github/labels.yml"
      }
    },
    "inclusive": {
      "secret": false,
      "vars": {
        "inclusiveOrg": "owner",
        "inclusiveTeam": "Community",
        "inclusiveComment": "Well done on getting your first Pull Request merged by our reviewers. The maintainers are proud of you!We would like to invite you to become a member of our github community team, which will allow you to get more access to our services & access to contributor only perks. Our bot should have already sent the invite. We look forward to you joining the team."
      }
    }
  }
}
